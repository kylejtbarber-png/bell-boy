{
  "expo": {
    "name": "bell-boy",
    "slug": "bell-boy",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "bellboy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bellboy.app"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bellboy.app",
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f9ac98d3-ddd5-4c30-a8b3-51381e6568e0"
      }
    }
  }
}
